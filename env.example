# Spotify API Configuration
# Получите эти ключи на https://developer.spotify.com/dashboard/applications
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
SPOTIFY_REDIRECT_URI=https://localhost:3000/callback

# Yandex.Music API Configuration
# Получите токен через: pip install yandex-music-token && yandex-music-token
YANDEX_MUSIC_TOKEN=your_yandex_music_token_here

# Application Configuration
LOG_LEVEL=INFO
SYNC_BATCH_SIZE=50
MAX_RETRIES=3
RETRY_DELAY=1

# Development Settings
DEBUG=false
ENVIRONMENT=development
